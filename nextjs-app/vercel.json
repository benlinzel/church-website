{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>.+)\\.winonagospel\\.ca"
        }
      ],
      "destination": "https://:subdomain.winonagospel.com/:1",
      "permanent": true
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "(?<subdomain>(?!www|library).+)\\.winonagospel\\.com"
        }
      ],
      "destination": "https://winonagospel.com/:1",
      "permanent": true
    }
  ]
}
